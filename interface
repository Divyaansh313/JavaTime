//new problem 

import java.util.*;

interface A {

	int a = 5;
	int b = 7;
	void meth1();
	void meth2();

}

class MyClass implements A{

public void meth1(){
	int sum = a+b;
	System.out.println("the sum is: " + sum);
}

public void meth2(){
	int diff = a-b;
	System.out.println("the difference is: " + diff);
}
}
